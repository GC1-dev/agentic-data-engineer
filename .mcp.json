{
  "mcpServers": {
    "databricks-utils": {
      "command": "./.venv/bin/python",
      "args": [
        "-m",
        "databricks_utils.mcp"
      ],
      "env": {
        "DATABRICKS_HOST": "${DATABRICKS_HOST}",
        "DATABRICKS_CONFIG_PROFILE": "${DATABRICKS_CONFIG_PROFILE}",
        "DATABRICKS_WAREHOUSE_ID": "${DATABRICKS_WAREHOUSE_ID}",
        "DATABRICKS_TOKEN": "dummy"
      }
    },
    "data-knowledge-base": {
      "command": ".venv/bin/python",
      "args": [
        "-m",
        "data_knowledge_base"
      ],
      "env": {
        "LOG_LEVEL": "INFO"
      }
    }
  }
}
